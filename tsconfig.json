    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "dist"
  },
  "filesGlob":[
    // "app/**/*.ts",
    // "!node_modules/**"
  ],
  "exclude": [
    "node_modules",
    "typings"
  ]
  
}
